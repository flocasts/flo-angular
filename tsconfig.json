{
  "compileOnSave": false,
  "exclude": [
    "projects/flosportsinc/ng-media-source-extensions/test"
  ],
  "compilerOptions": {
    "resolveJsonModule": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@flosportsinc/ng-viewport-grid": [
        "dist/flosportsinc/ng-viewport-grid"
      ],
      "@flosportsinc/ng-viewport-grid/*": [
        "dist/flosportsinc/ng-viewport-grid/*"
      ],
      "@flosportsinc/ng-media-source-extensions": [
        "dist/flosportsinc/ng-media-source-extensions"
      ],
      "@flosportsinc/ng-media-source-extensions/*": [
        "dist/flosportsinc/ng-media-source-extensions/*"
      ],
      "@flosportsinc/ng-universal-services": [
        "dist/flosportsinc/ng-universal-services"
      ],
      "@flosportsinc/ng-universal-services/*": [
        "dist/flosportsinc/ng-universal-services/*"
      ],
      "@flosportsinc/ng-icons": [
        "dist/flosportsinc/ng-icons"
      ],
      "@flosportsinc/ng-icons/*": [
        "dist/flosportsinc/ng-icons/*"
      ],
      "@flosportsinc/ng-svg-transfer-state": [
        "dist/flosportsinc/ng-svg-transfer-state"
      ],
      "@flosportsinc/ng-svg-transfer-state/*": [
        "dist/flosportsinc/ng-svg-transfer-state/*"
      ],
      "@flosportsinc/ng-ad-block": [
        "dist/flosportsinc/ng-ad-block"
      ],
      "@flosportsinc/ng-ad-block/*": [
        "dist/flosportsinc/ng-ad-block/*"
      ],
      "@flosportsinc/ng-http-cache-tags": [
        "dist/flosportsinc/ng-http-cache-tags"
      ],
      "@flosportsinc/ng-http-cache-tags/*": [
        "dist/flosportsinc/ng-http-cache-tags/*"
      ],
      "@flosportsinc/ng-window": [
        "dist/flosportsinc/ng-window"
      ],
      "@flosportsinc/ng-window/*": [
        "dist/flosportsinc/ng-window/*"
      ],
      "@flosportsinc/ng-env-transfer-state": [
        "dist/flosportsinc/ng-env-transfer-state"
      ],
      "@flosportsinc/ng-env-transfer-state/*": [
        "dist/flosportsinc/ng-env-transfer-state/*"
      ]
    }
  }
}