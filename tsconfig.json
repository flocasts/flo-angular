{
  "compileOnSave": false,
  "compilerOptions": {
    "resolveJsonModule": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@flosportsinc/ng-viewport-grid": [
        "dist/flosportsinc/ng-viewport-grid"
      ],
      "@flosportsinc/ng-viewport-grid/*": [
        "dist/flosportsinc/ng-viewport-grid/*"
      ],
      "@flosportsinc/ng-media-source-extensions": [
        "dist/flosportsinc/ng-media-source-extensions"
      ],
      "@flosportsinc/ng-media-source-extensions/*": [
        "dist/flosportsinc/ng-media-source-extensions/*"
      ],
      "@flosportsinc/ng-universal-services": [
        "dist/flosportsinc/ng-universal-services"
      ],
      "@flosportsinc/ng-universal-services/*": [
        "dist/flosportsinc/ng-universal-services/*"
      ],
      "@flosportsinc/ng-media-source-extensions-dash": [
        "dist/flosportsinc/ng-media-source-extensions-dash"
      ],
      "@flosportsinc/ng-media-source-extensions-dash/*": [
        "dist/flosportsinc/ng-media-source-extensions-dash/*"
      ],
      "@flosportsinc/ng-media-source-extensions-hls": [
        "dist/flosportsinc/ng-media-source-extensions-hls"
      ],
      "@flosportsinc/ng-media-source-extensions-hls/*": [
        "dist/flosportsinc/ng-media-source-extensions-hls/*"
      ]
    }
  }
}