{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "commonjs",
    "types": [
      "node"
    ],
    "paths": {
      "@flosportsinc/ng-viewport-grid": [
        "../dist/flosportsinc/ng-viewport-grid"
      ],
      "@flosportsinc/ng-viewport-grid/*": [
        "../dist/flosportsinc/ng-viewport-grid/*"
      ],
      "@flosportsinc/ng-hls": [
        "../dist/flosportsinc/ng-hls"
      ],
      "@flosportsinc/ng-hls/*": [
        "../dist/flosportsinc/ng-hls/*"
      ],
      "@flosportsinc/ng-universal-services": [
        "../dist/flosportsinc/ng-universal-services"
      ],
      "@flosportsinc/ng-universal-services/*": [
        "../dist/flosportsinc/ng-universal-services/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "entryModule": "app/app.server.module#AppServerModule"
  }
}