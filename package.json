    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.4.0",
    "protractor": "^5.4.1",
    "ts-node": "~7.0.1",
    "tsickle": "^0.33.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "tslint-immutable": "^4.8.0",
    "typescript": "~3.1.3"
  },
  "engines": {
    "node": "= 10.12.0",
    "npm": "= 6.4.1"
  }
