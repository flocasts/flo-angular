{
  "name": "flo-angular",
  "version": "0.0.0-development",
  "private": false,
  "author": "Patrick Michalina <patrickmichalina@mac.com>",
  "contributors": [
    "Mattthew Elliott <matthew.elliott@flosports.tv>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/flocasts/flo-angular"
  },
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 10"
  ],
  "scripts": {
    "postshrinkwrap": "if [ \"`uname`\" = \"Darwin\" ]; then sed -i '' -e 's/http:\\/\\//https:\\/\\//g' package-lock.json; else sed -i -e 's/http:\\/\\//https:\\/\\//g' package-lock.json; fi",
    "ng": "ng",
    "start": "ng serve",
    "start.ssr": "npm run build.ssr && node -r dotenv/config dist/server.js",
    "start.prod": "npm run build.prod && node dist/server.js",
    "copy": "ts-node --project tsconfig.tools.json tools/copy.ts",
    "styles.scss": "npm run styles.scss.full && npm run styles.scss.min",
    "styles.scss.full": "node-sass projects/flosportsinc/ng-styles/common/styles/main.scss dist/flosportsinc/ng-styles/css/flo.css",
    "styles.scss.min": "node-sass projects/flosportsinc/ng-styles/common/styles/main.scss dist/flosportsinc/ng-styles/css/flo.min.css --output-style=compressed",
    "build": "ng build --service-worker",
    "build.lib": "ts-node --project tsconfig.tools.json tools/build.ts && npm run copy && npm run styles.scss",
    "build.ssr": "ng build && ng run flo-angular:node && npm run webpack.server",
    "build.prod": "ng build --prod && ts-node --project tsconfig.tools.json tools/compress.ts && ng run flo-angular:node && npm run webpack.server",
    "test": "ng test",
    "test.ci.app": "ng test flo-angular --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.mse": "ng test @flosportsinc/ng-media-source-extensions --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.services": "ng test @flosportsinc/ng-universal-services --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.icons": "ng test @flosportsinc/ng-icons --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.styles": "ng test @flosportsinc/ng-styles --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.svg-transfer-state": "ng test @flosportsinc/ng-svg-transfer-state --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.ad-block": "ng test @flosportsinc/ng-ad-block --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.http-cache-tags": "ng test @flosportsinc/ng-http-cache-tags --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.ima": "ng test @flosportsinc/ng-ima --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.tabs": "ng test @flosportsinc/ng-tabs --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.video-autoplay": "ng test @flosportsinc/ng-video-autoplay --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.video-events": "ng test @flosportsinc/ng-video-events --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test.ci.grid-list": "ng test @flosportsinc/ng-grid-list --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "lint": "ts-node --project tsconfig.tools.json node_modules/.bin/ng lint",
    "e2e": "ng e2e",
    "e2e.ci": "ng e2e --dev-server-target= --protractor-config=./e2e/protractor-ci.conf.js",
    "e2e.ssr": "ng e2e --dev-server-target= --protractor-config=./e2e/protractor-ssr.conf.js",
    "e2e.prod": "ng e2e --dev-server-target= --protractor-config=./e2e/protractor-ssr.conf.js",
    "heroku-postbuild": "npm run build.lib && npm run build.prod",
    "webpack.server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "dependencies": {
    "@angular/animations": "8.0.0-beta.11",
    "@angular/common": "8.0.0-beta.11",
    "@angular/compiler": "8.0.0-beta.11",
    "@angular/core": "8.0.0-beta.11",
    "@angular/forms": "8.0.0-beta.11",
    "@angular/http": "8.0.0-beta.10",
    "@angular/platform-browser": "8.0.0-beta.11",
    "@angular/platform-browser-dynamic": "8.0.0-beta.11",
    "@angular/platform-server": "8.0.0-beta.11",
    "@angular/pwa": "^0.13.9",
    "@angular/router": "8.0.0-beta.11",
    "@angular/service-worker": "8.0.0-beta.11",
    "@nguniversal/common": "^7.1.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.9",
    "dashjs": "^2.9.3",
    "express": "^4.17.1",
    "express-static-gzip": "^1.1.3",
    "hls.js": "^0.12.4",
    "js-cookie": "^2.2.0",
    "lru-cache": "^5.1.1",
    "ngx-markdown": "^7.1.5",
    "rxjs": "6.4.0",
    "stream": "0.0.2",
    "typescript-monads": "^3.13.0",
    "uuid": "^3.3.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular-devkit/build-ng-packagr": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "8.0.0-beta.11",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@semantic-release/exec": "^3.3.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/glob": "^7.1.1",
    "@types/hls.js": "^0.12.4",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.6",
    "@types/js-cookie": "^2.2.2",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^12.0.7",
    "@types/shelljs": "^0.8.5",
    "@types/uuid": "^3.4.4",
    "codecov": "^3.5.0",
    "codelyzer": "^5.1.0",
    "dotenv": "^7.0.0",
    "glob": "^7.1.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.3.0",
    "protractor": "^5.4.2",
    "semantic-release": "^15.13.12",
    "shelljs": "^0.8.3",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.2.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.3",
    "tslint": "^5.17.0",
    "tslint-immutable": "^6.0.0",
    "typescript": "3.3.3333",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  },
  "release": {
    "branch": "master",
    "npmPublish": false,
    "plugins": [
      [
        "@semantic-release/exec",
        {
          "successCmd": "ts-node --project tsconfig.tools.json tools/publish.ts ver=${nextRelease.version}"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ]
    ]
  },
  "engines": {
    "node": "= 12.4.0"
  }
}
